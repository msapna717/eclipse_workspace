package com.collect.test;

import java.util.ArrayList;
import java.util.List;

public class ArraylistTest {
	public static void main(String[] args) {
		ArrayList al1=new ArrayList();
		System.out.println("List capacity: "+al1.size());
		//add(object)
		al1.add(10);
		al1.add(20);
		al1.add(30);
		al1.add("abc");
		al1.add("Game");
		System.out.println(al1);
		//add(index,object)
		al1.add(0, "hello");
		System.out.println(al1);
		ArrayList al2=new ArrayList();
		al2.add(40);
		al2.add(50);
		al2.add(60);
		System.out.println(al2);
		//addAll(object)
		al2.addAll(al1);
		System.out.println(al2);
		//addAll(index,object)
		al2.addAll(2, al1);
		System.out.println(al2);
		ArrayList al3=new ArrayList();
		//clone()
		al3=(ArrayList) al2.clone();
		System.out.println(al3);
		//contains(object)
		System.out.println(al2.contains(40));
		System.out.println(al2);
		//containsAll(collection c)
		System.out.println(al2.containsAll(al1));
		//isEmpty()
		System.out.println(al2.isEmpty());
		//clear()
		al2.clear();
		System.out.println(al2);
		System.out.println(al2.isEmpty());
	
		}
}
