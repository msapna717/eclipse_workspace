package com.collect.test;

import java.util.LinkedList;
import java.util.List;

public class LinkedlistTest {
	
	public static void main(String[] args) {
		List ll=new LinkedList();
		//add(object)
		ll.add(10);
		ll.add(20);
		ll.add(30);
		ll.add(40);
		ll.add(50);
		//add(index,object)
		ll.add(2, 50);
		System.out.println("Linked list ll is: "+ll);

		LinkedList ll1=new LinkedList();
		//addAll(collection c)
		ll1.addAll(ll);
		System.out.println("Linked list ll1 is: "+ll1);
		//addFirst(object)
		ll1.addFirst(1);
		System.out.println("Linked list ll1 is: "+ll1);
		//addLast(object) 
		ll1.addLast(2);
		System.out.println("Linked list ll1 is: "+ll1);
		//get(index)
		System.out.println(ll1.get(5));
		//getFirst()
		System.out.println("First value of ll1: "+ll1.getFirst());
		//getLast()
		System.out.println("Last value of ll1: "+ll1.getLast());
		//indexOf(object o)
		System.out.println("index of 50 is: "+ll1.indexOf(50));
		//lastIndexOf(object o)
		System.out.println(ll1.lastIndexOf(50));
		//peek()//returns head
		System.out.println(ll1.peek());
		//peekFirst()//returns first
		System.out.println(ll1.peekFirst());
		//peekLast()//returns last
		System.out.println(ll1.peekLast());
		//offer(object 0)// adds at end
		ll1.offer(60);
		System.out.println("Linked list ll1 is:"+ll1);
		//offerFirst(object 0)// adds at first
		ll1.offerFirst(70);
		System.out.println("Linked list ll1 is:"+ll1);
		//offerLast(object 0)// adds at last
		ll1.offerLast(80);
		System.out.println("Linked list ll1 is:"+ll1);
		//poll()
		System.out.println(ll1.poll());
		System.out.println("Linked list ll1 is:"+ll1);
		System.out.println(ll1.pollFirst());
		System.out.println("Linked list ll1 is:"+ll1);
		
		LinkedList ll2=new LinkedList();
		ll2.add(6);
		ll2.addAll(ll1);
		//getFirst()
		System.out.println("First value of ll2: "+ll2.getFirst());
		System.out.println("Linked list ll2 is: "+ll2);
//		//addAll(index,collection c)
//		ll2.addAll(2, ll1);
//		System.out.println("Linked list ll2 is: "+ll2);
		
		
	}

}
